                                        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
                                      </장소>
                                    </단체_장소>
                                    <단체_인물 rdf:resource="http://example.org/MIH_O/박용길"/>
                                    <단체_사건>
                                      <사건 rdf:about="http://example.org/MIH_O/피난">
